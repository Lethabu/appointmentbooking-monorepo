                },
                {
                    "id": "compliance-status",
                    "type": "compliance_dashboard",
                    "position": {
                        "x": 6,
                        "y": 6,
                        "width": 6,
                        "height": 2
                    },
                    "title": "Compliance Status",
                    "standards": [
