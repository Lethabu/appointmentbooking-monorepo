{
    "dashboard_templates": {
        "executive_overview_dashboard": {
            "name": "Executive Overview Dashboard",
            "description": "High-level business and technical metrics for executive decision making",
            "target_audience": "C-Level Executives, Business Owners",
            "refresh_interval": "1h",
            "layout": {
                "grid_columns": 12,
                "grid_rows": 8,
                "responsive": true
            },
            "widgets": [
                {
                    "id": "revenue-overview",
                    "type": "metric_card",
                    "position": {
                        "x": 0,
                        "y": 0,
                        "width": 6,
                        "height": 2
                    },
                    "title": "Revenue Overview",
                    "metrics": [
                        {
                            "label": "Daily Revenue",
                            "value": "current_daily_revenue",
                            "target": "daily_revenue_target",
                            "format": "currency",
                            "currency": "ZAR"
                        },
                        {
                            "label": "Monthly Revenue",
                            "value": "current_monthly_revenue",
                            "target": "monthly_revenue_target",
                            "format": "currency",
                            "currency": "ZAR"
                        },
                        {
                            "label": "Revenue Growth",
                            "value": "revenue_growth_rate",
                            "format": "percentage",
                            "trend": true
                        }
                    ]
                },
                {
                    "id": "conversion-summary",
                    "type": "gauge_chart",
                    "position": {
                        "x": 6,
                        "y": 0,
                        "width": 3,
                        "height": 2
                    },
                    "title": "Conversion Rate",
                    "metric": "overall_conversion_rate",
                    "targets": {
                        "target": 0.15,
                        "warning": 0.12,
                        "critical": 0.10
                    },
                    "format": "percentage"
                },
                {
                    "id": "system-health",
                    "type": "status_indicators",
                    "position": {
                        "x": 9,
                        "y": 0,
                        "width": 3,
                        "height": 2
                    },
                    "title": "System Health",
                    "indicators": [
                        {
                            "label": "Uptime",
                            "value": "system_uptime",
                            "status": "uptime_status",
                            "target": 99.9
                        },
                        {
                            "label": "Response Time",
                            "value": "avg_response_time",
                            "status": "response_time_status",
                            "target": 200
                        },
                        {
                            "label": "Error Rate",
                            "value": "error_rate",
                            "status": "error_rate_status",
                            "target": 0.001
                        }
                    ]
                },
                {
                    "id": "revenue-trend",
                    "type": "line_chart",
                    "position": {
                        "x": 0,
                        "y": 2,
                        "width": 8,
                        "height": 3
                    },
                    "title": "Revenue Trend (Last 30 Days)",
                    "data_source": "revenue_analytics",
                    "metrics": [
                        "daily_revenue"
                    ],
                    "time_range": "30d",
                    "interpolation": "smooth",
                    "show_target_line": true,
                    "targets": {
                        "target_line": "daily_revenue_target"
                    }
                },
                {
                    "id": "booking-funnel",
                    "type": "funnel_chart",
                    "position": {
                        "x": 8,
                        "y": 2,
                        "width": 4,
                        "height": 3
                    },
                    "title": "Booking Conversion Funnel",
                    "funnel_stages": [
                        {
                            "label": "Landing Page Views",
                            "value": "landing_page_views"
                        },
                        {
                            "label": "Booking Page Visits",
                            "value": "booking_page_visits"
                        },
                        {
                            "label": "Service Selections",
                            "value": "service_selections"
                        },
                        {
                            "label": "Payment Initiated",
                            "value": "payment_initiated"
                        },
                        {
                            "label": "Bookings Completed",
                            "value": "bookings_completed"
                        }
                    ],
                    "show_conversion_rates": true,
                    "highlight_drop_offs": true
                },
                {
                    "id": "top-services",
                    "type": "bar_chart",
                    "position": {
                        "x": 0,
                        "y": 5,
                        "width": 6,
                        "height": 3
                    },
                    "title": "Top Performing Services",
                    "data_source": "service_analytics",
                    "metrics": [
                        "revenue_by_service",
                        "bookings_by_service"
                    ],
                    "chart_type": "horizontal_bar",
                    "limit": 5,
                    "show_values": true
                },
                {
                    "id": "customer-satisfaction",
                    "type": "gauge_chart",
                    "position": {
                        "x": 6,
                        "y": 5,
                        "width": 3,
                        "height": 3
                    },
                    "title": "Customer Satisfaction",
                    "metric": "customer_satisfaction_score",
                    "targets": {
                        "target": 4.5,
                        "warning": 4.0,
                        "critical": 3.5
                    },
                    "format": "decimal",
                    "max": 5
                },
                {
                    "id": "system-performance",
                    "type": "line_chart",
                    "position": {
                        "x": 9,
                        "y": 5,
                        "width": 3,
                        "height": 3
                    },
                    "title": "System Performance (24h)",
                    "metrics": [
                        "response_time_p95",
                        "error_rate",
                        "throughput"
                    ],
                    "time_range": "24h",
                    "dual_axis": true,
                    "show_thresholds": true
                }
            ],
            "kpi_cards": [
                {
                    "title": "Monthly Revenue Target",
                    "value": "monthly_revenue",
                    "target": "monthly_revenue_target",
                    "format": "currency",
                    "currency": "ZAR",
                    "trend": true
                },
                {
                    "title": "Customer Acquisition Cost",
                    "value": "customer_acquisition_cost",
                    "format": "currency",
                    "currency": "ZAR",
                    "trend": false
                },
                {
                    "title": "Average Booking Value",
                    "value": "average_booking_value",
                    "format": "currency",
                    "currency": "ZAR",
                    "trend": true
                },
                {
                    "title": "Customer Retention Rate",
                    "value": "customer_retention_rate",
                    "format": "percentage",
                    "trend": true
                }
            ]
        },
        "operations_dashboard": {
            "name": "Operations Dashboard",
            "description": "Real-time operational metrics and system health monitoring",
            "target_audience": "Operations Team, DevOps Engineers",
            "refresh_interval": "30s",
            "layout": {
                "grid_columns": 12,
                "grid_rows": 10,
                "responsive": true
            },
            "widgets": [
                {
                    "id": "system-status",
                    "type": "status_grid",
                    "position": {
                        "x": 0,
                        "y": 0,
                        "width": 12,
                        "height": 2
                    },
                    "title": "System Status Overview",
                    "systems": [
                        {
                            "name": "Cloudflare Workers",
                            "status": "worker_status",
                            "response_time": "worker_response_time",
                            "error_rate": "worker_error_rate"
                        },
                        {
                            "name": "Database (D1)",
                            "status": "database_status",
                            "response_time": "database_response_time",
                            "connection_count": "active_connections"
                        },
                        {
                            "name": "CDN",
                            "status": "cdn_status",
                            "cache_hit_rate": "cache_hit_rate",
                            "bandwidth": "bandwidth_usage"
                        },
                        {
                            "name": "Payment Gateway",
                            "status": "payment_status",
                            "transaction_rate": "payment_transaction_rate",
                            "failure_rate": "payment_failure_rate"
                        }
                    ]
                },
                {
                    "id": "realtime-traffic",
                    "type": "line_chart",
                    "position": {
                        "x": 0,
                        "y": 2,
                        "width": 8,
                        "height": 3
                    },
                    "title": "Real-time Traffic (Last 24h)",
                    "metrics": [
                        "requests_per_minute",
                        "concurrent_users",
                        "bandwidth_usage"
                    ],
                    "time_range": "24h",
                    "real_time": true,
                    "show_baselines": true
                },
                {
                    "id": "error-analysis",
                    "type": "error_breakdown",
                    "position": {
                        "x": 8,
                        "y": 2,
                        "width": 4,
                        "height": 3
                    },
                    "title": "Error Analysis (24h)",
                    "breakdown": [
                        "4xx_errors",
                        "5xx_errors",
                        "timeout_errors"
                    ],
                    "show_stack_traces": true,
                    "error_tracking": "sentry"
                },
                {
                    "id": "performance-metrics",
                    "type": "multi_metric_chart",
                    "position": {
                        "x": 0,
                        "y": 5,
                        "width": 6,
                        "height": 2
                    },
                    "title": "Performance Metrics (1h)",
                    "metrics": [
                        {
                            "name": "Response Time P95",
                            "value": "response_time_p95",
                            "unit": "ms"
                        },
                        {
                            "name": "Database Queries",
                            "value": "database_query_time",
                            "unit": "ms"
                        },
                        {
                            "name": "External API Calls",
                            "value": "external_api_latency",
                            "unit": "ms"
                        }
                    ],
                    "time_range": "1h",
                    "show_percentiles": true
                },
                {
                    "id": "resource-utilization",
                    "type": "resource_gauge",
                    "position": {
                        "x": 6,
                        "y": 5,
                        "width": 3,
                        "height": 2
                    },
                    "title": "Resource Utilization",
                    "resources": [
                        {
                            "name": "CPU Usage",
                            "value": "cpu_usage",
                            "max": 100
                        },
                        {
                            "name": "Memory Usage",
                            "value": "memory_usage",
                            "max": 128
                        },
                        {
                            "name": "Storage Usage",
                            "value": "storage_usage",
                            "max": 1000
                        }
                    ],
                    "format": "percentage"
                },
                {
                    "id": "security-events",
                    "type": "security_events_table",
                    "position": {
                        "x": 9,
                        "y": 5,
                        "width": 3,
                        "height": 2
                    },
                    "title": "Security Events (24h)",
                    "events": [
                        "blocked_requests",
                        "ddos_attempts",
                        "auth_failures"
                    ],
                    "show_geo_data": true,
                    "threat_level_indicators": true
                },
                {
                    "id": "infrastructure-health",
                    "type": "health_check_grid",
                    "position": {
                        "x": 0,
                        "y": 7,
                        "width": 12,
                        "height": 3
                    },
                    "title": "Infrastructure Health Check",
                    "checks": [
                        {
                            "name": "API Endpoints",
                            "checks": [
                                {
                                    "endpoint": "/api/health",
                                    "expected": 200,
                                    "timeout": 5000
                                },
                                {
                                    "endpoint": "/api/bookings",
                                    "expected": 200,
                                    "timeout": 10000
                                },
                                {
                                    "endpoint": "/api/payments",
                                    "expected": 200,
                                    "timeout": 15000
                                }
                            ]
                        },
                        {
                            "name": "External Services",
                            "checks": [
                                {
                                    "service": "Paystack",
                                    "endpoint": "api.paystack.co",
                                    "timeout": 10000
                                },
                                {
                                    "service": "Google Calendar",
                                    "endpoint": "www.googleapis.com",
                                    "timeout": 5000
                                },
                                {
                                    "service": "WhatsApp Business",
                                    "endpoint": "graph.facebook.com",
                                    "timeout": 10000
                                }
                            ]
                        },
                        {
                            "name": "Database",
                            "checks": [
                                {
                                    "check": "Connection Pool",
                                    "metric": "active_connections",
                                    "max": 100
                                },
                                {
                                    "check": "Query Performance",
                                    "metric": "avg_query_time",
                                    "max": 100
                                },
                                {
                                    "check": "Storage Health",
                                    "metric": "storage_usage",
                                    "max": 90
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        "security_operations_dashboard": {
            "name": "Security Operations Dashboard",
            "description": "Comprehensive security monitoring and threat detection",
            "target_audience": "Security Team, SOC Analysts",
            "refresh_interval": "1m",
            "layout": {
                "grid_columns": 12,
                "grid_rows": 8,
                "responsive": true
            },
            "widgets": [
                {
                    "id": "threat-overview",
