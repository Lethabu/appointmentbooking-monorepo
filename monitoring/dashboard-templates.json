                    "position": {
                        "x": 0,
                        "y": 4,
                        "width": 12,
                        "height": 2
                    },
                    "title": "Security Incidents Timeline (24h)",
                    "events": "security_incidents",
                    "severity_indicators": true,
                    "auto_refresh": true
                },
                {
                    "id": "vulnerability-assessment",
                    "type": "vuln_status",
                    "position": {
                        "x": 0,
                        "y": 6,
                        "width": 6,
                        "height": 2
                    },
                    "title": "Vulnerability Assessment",
                    "categories": [
                        {
                            "name": "Critical",
