    "dev": "next dev",
    "build": "next build",
    "pages:build": "next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test e2e/smoke-tests.spec.ts",
    "compile-scripts": "tsc scripts/bootstrapTenant.ts --outDir dist --target es2020 --module commonjs --esModuleInterop",
    "bootstrap-tenant": "npm run compile-scripts && node dist/scripts/bootstrapTenant.js",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "npm run pages:deploy",
    "deploy:all": "npm run deploy:worker && npm run deploy:pages",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "cf:dev": "wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "cf:preview": "npm run pages:build && wrangler pages deploy .vercel/output/static --compatibility-date=2024-01-15",
    "analyze": "ANALYZE=true next build",
    "analyze:view": "npm run analyze && npx webpack-bundle-analyzer .next/static/chunks/stats.json"
